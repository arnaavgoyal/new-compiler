#ifndef REG
#define REG(val, bits)
#endif

#ifndef SUBREG
#define SUBREG(val, bits, parent) REG(val, bits)
#endif

REG(rax, 64)
REG(rbx, 64)
REG(rcx, 64)
REG(rdx, 64)
REG(rsi, 64)
REG(rdi, 64)
REG(rbp, 64)
REG(rsp, 64)
