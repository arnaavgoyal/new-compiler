type my_i32 = i32;

def (u8) *i8
foo(num) {
    return num;
}

def (*i8) i8
bar(str) {
    var i8 v = *(str + foo(1));
    return v;
}

def (i32, **i8) i8
my_main(argc, argv) {
    var i8 v = 3;
    v = bar(*argv);
    return v;
}

