def (*i8) i32
foo(str) {
    return 0;
}
def (i32, **i8) i32
main(argc, argv) {
    var *i8 str = *argv;
    var i32 val = 0;
    while (val) {
        val = foo(str);
        if (val) {
            val = 1;
        }
        else {
            val = 4;
        }
    }
    if (argc == 0) {
        str = *argv;
        val = foo(*argv);
    }
    else if (argc == 2) {
        str = *argv;
    }
    foo(str);
    return val;
}

